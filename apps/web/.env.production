# Production Environment Variables
VITE_API_URL=https://api.ansvar.com
VITE_WS_URL=wss://api.ansvar.com
VITE_APP_NAME=Ansvar Threat Modeling
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production

# Feature Flags
VITE_ENABLE_WEBSOCKET=true
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_DEBUG=false

# Security
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=application/pdf,text/plain,text/markdown,application/vnd.openxmlformats-officedocument.wordprocessingml.document

# Performance
VITE_API_TIMEOUT=30000
VITE_CACHE_TTL=600000
VITE_RETRY_ATTEMPTS=3